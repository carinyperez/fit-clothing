m '../../redux/cart/cart.actions';\n\n// reference to anon function that takes props and renders component\n// pulling data from shop data \n\nclass CollectionItem extends React.Component {\n    constructor(props) {\n        super(props); \n        this.state = {\n            backgroundImage: `url(${props.item.imageUrl})`, \n            hidden: true, \n        }\n    }\n   handleMouseEnter = () => {\n    const {changeImageUrl} = this.props.item; \n    this.setState({\n        backgroundImage: `url(${changeImageUrl})`, \n        hidden: false\n    \n      });\n    };\n    handleMouseLeave = () => {\n    const {imageUrl} = this.props.item; \n    this.setState({\n        backgroundImage: `url(${imageUrl})`,\n        hidden: true\n        });\n    };\n    handleSubmit = (event) => {\n        const {toggleCartVisible} = this.props; \n        event.preventDefault();\n        toggleCartVisible();\n        // setTimeout(toggleCartVisible(), 5000);\n        // on submit popup checkout cart \n    } \n\n    render () {\n        const {hidden} = this.state;\n        const {item, addItem} = this.props;   \n        return (\n        <div className='collection-item'\n            onMouseEnter={this.handleMouseEnter}\n            onMouseLeave={this.handleMouseLeave}\n        >\n            <div className='image' \n            style={{\n                backgroundImage: this.state.backgroundImage\n            }}\n            \n            />\n            <div className='quickadd'>\n            {\n            hidden ? null : \n            <div className='quickadd-content'>\n            <form onSubmit={this.handleSubmit}>\n                <p>Quick Add</p>\n                <button onClick={() => addItem(item)}>S</button>\n                <button type='submit' onClick={() => addItem(item)}>M</button>\n                <button type='submit' onClick={() => addItem(item)}>L</button>\n                <button type='submit' onClick={() => addItem(item)}>XL</button>\n            </form>\n            </div>\n            }\n            </div>\n            <div className='collection-footer'>\n                <span className='name'>{item.name}</span>\n                <span className='price'>{item.price}</span>\n            </div>\n        </div>\n        ); \n    }\n}\n\nconst mapDispatchToProps = dispatch => ({\n    addItem: item => dispatch(addItem(item)),\n    toggleCartVisible: () => dispatch(toggleCartVisible()),\n})\n\nexport default connect(null,mapDispatchToProps)(CollectionItem);\n\n","/Users/cariny/Desktop/fit-clothing/src/components/header/header.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/pages/sign-in-and-signup/sign-in-and-sign-up.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/components/sign-in/sign-in.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/firebase/firebase.utils.js",[],"/Users/cariny/Desktop/fit-clothing/src/components/custom-button/custom-button.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/components/form-input/form-input.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/components/sign-up/sign-up.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/redux/store.js",[],"/Users/cariny/Desktop/fit-clothing/src/redux/root-reducer.js",[],"/Users/cariny/Desktop/fit-clothing/src/redux/user/user.reducer.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/redux/user/user.actions.js",[],"/Users/cariny/Desktop/fit-clothing/src/redux/user/user.types.js",[],"/Users/cariny/Desktop/fit-clothing/src/components/cart-icon/cart-icon.comoponent.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/components/cart-dropdown/cart-dropdown.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/redux/cart/cart.actions.js",[],"/Users/cariny/Desktop/fit-clothing/src/redux/cart/cart.reducer.js",["132"],"import { addItem } from './cart.actions';\nimport CartActionTypes from './cart.types'; \nimport {addItemToCart} from './cart.utils'; \n// we use reducers when we want to move our state into a global object \n// initial state is an object literal with the property hidden \nconst INITIAL_STATE = {\n    hidden: true, \n    cartItems: []\n}\n\nconst cartReducer = (state = INITIAL_STATE, action) => {\n    switch(action.type) {\n        case CartActionTypes.TOGGLE_CART_HIDDEN: \n        return {\n            ...state, \n            hidden: !state.hidden\n        }\n        case CartActionTypes.TOGGLE_CART_VISIBLE: \n        return {\n            ...state, \n            hidden: false\n        }\n\n        case CartActionTypes.ADD_ITEM: \n        return  {\n            ...state,\n            cartItems: addItemToCart(state.cartItems, action.payload) \n            // cartItems: [...state.cartItems, action.payload]\n        }\n\n\n        default:\n            return state;\n\n    }\n}\n\nexport default cartReducer;\n\n","/Users/cariny/Desktop/fit-clothing/src/redux/cart/cart.types.js",[],"/Users/cariny/Desktop/fit-clothing/src/redux/cart/cart.utils.js",[],"/Users/cariny/Desktop/fit-clothing/src/components/cart-item/cart-item.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/redux/cart/cart.selectors.js",[],{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","severity":1,"message":"138","line":4,"column":37,"nodeType":"139","messageId":"140","endLine":4,"endColumn":53},{"ruleId":"137","severity":1,"message":"141","line":1,"column":10,"nodeType":"139","messageId":"140","endLine":1,"endColumn":17},"no-native-reassign",["142"],"no-negated-in-lhs",["143"],"no-unused-vars","'toggleCartHidden' is defined but never used.","Identifier","unusedVar","'addItem' is defined but never used.","no-global-assign","no-unsafe-negation"]