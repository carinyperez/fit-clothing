             backgroundImage: this.state.backgroundImage\n            }}\n            \n            />\n            <div className='quickadd'>\n            {\n            hidden ? null : \n            <div className='quickadd-content'>\n            <form onSubmit={this.handleSubmit}>\n                <p>Quick Add</p>\n                <button onClick={() => addItem(item)}>S</button>\n                <button type='submit' onClick={() => addItem(item)}>M</button>\n                <button type='submit' onClick={() => addItem(item)}>L</button>\n                <button type='submit' onClick={() => addItem(item)}>XL</button>\n            </form>\n            </div>\n            }\n            </div>\n            <div className='collection-footer'>\n                <span className='name'>{item.name}</span>\n                <span className='price'>${item.price}</span>\n            </div>\n        </div>\n        ); \n    }\n}\n\nconst mapDispatchToProps = dispatch => ({\n    addItem: item => dispatch(addItem(item))\n})\n\nexport default connect(null,mapDispatchToProps)(CollectionItem);\n\n","/Users/cariny/Desktop/fit-clothing/src/components/header/header.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/pages/sign-in-and-signup/sign-in-and-sign-up.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/components/sign-in/sign-in.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/firebase/firebase.utils.js",[],"/Users/cariny/Desktop/fit-clothing/src/components/custom-button/custom-button.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/components/form-input/form-input.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/components/sign-up/sign-up.component.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/redux/store.js",[],"/Users/cariny/Desktop/fit-clothing/src/redux/root-reducer.js",[],"/Users/cariny/Desktop/fit-clothing/src/redux/user/user.reducer.jsx",[],"/Users/cariny/Desktop/fit-clothing/src/redux/user/user.actions.js",[],"/Users/cariny/Des